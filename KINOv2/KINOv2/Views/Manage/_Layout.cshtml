@{
    Layout = "/Views/Shared/_Layout.cshtml";
}

<div class="container px-0 px-sm-3">
    <div class="row">
        <div class="col-12">
            <div class="d-md-flex flex-md-row mt-0">
                @await Html.PartialAsync("_ManageNav")

                <div class="tab-content py-3 px-md-3 py-md-0 w-100 mt-2">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}

